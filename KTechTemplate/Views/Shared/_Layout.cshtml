<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="format-detection" />

    <title>@ViewData["Title"] - KTechTemplate App</title>

    <link rel="stylesheet" href="~/lib/fontawesome/css/all.min.css" />
	<link rel="stylesheet" href="~/lib/dm-file-uploader/css/jquery.dm-uploader.min.css" />
    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css" />
    <link rel="stylesheet" href="//cdn.datatables.net/1.12.1/css/jquery.dataTables.min.css"/>

    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/fontawesome/js/all.min.js"></script>
    <script src="~/lib/bs-custom-file-input/bs-custom-file-input.min.js"></script>
	<script src="~/lib/dm-file-uploader/js/jquery.dm-uploader.min.js"></script>
    <script src="~/js/tinymce/tinymce.min.js"></script>
    <script src="~/js/site.min.js" asp-append-version="true"></script>
    <script src="~/js/product.js"></script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="//cdn.datatables.net/1.12.1/js/jquery.dataTables.min.js"></script>

    <link rel="apple-touch-icon" sizes="180x180" href="/images/logo-inverse.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/images/logo-inverse.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/images/logo-inverse.png">
    <link rel="manifest" href="/site.webmanifest">
</head>


<body class="d-flex flex-column" style="background-color:#e7eaef">
    <div style="z-index: 20">
        <partial name="_Notifications" />
        <partial name="_Nav" /><br />
    </div>
    <div id="page-content">
        <div class="container-fluid">
            <div class="row m-2">
                <div class="col-lg-12 pt-2  shadow rounded-lg bg-white">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <partial name="_Footer" />
</body>
@RenderSection("Scripts", required: false)

</html>
